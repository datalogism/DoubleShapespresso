@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://shaclshapes.org/BuildingShape> a sh:NodeShape ;
    sh:property [ sh:datatype xsd:double ;
            sh:path <http://dbpedia.org/ontology/height> ],
        [ sh:class <http://dbpedia.org/ontology/Place> ;
            sh:path <http://dbpedia.org/ontology/location> ],
        [ sh:class <http://dbpedia.org/ontology/Agent> ;
            sh:path <http://dbpedia.org/ontology/owner> ],
        [ sh:path <http://dbpedia.org/ontology/isPartOf> ],
        [ sh:class <http://dbpedia.org/ontology/Organisation> ;
            sh:path <http://dbpedia.org/ontology/tenant> ],
        [ sh:datatype xsd:positiveInteger ;
            sh:path <http://dbpedia.org/ontology/floorCount> ],
        [ sh:path <http://dbpedia.org/ontology/addedToTheNationalRegisterOfHistoricPlaces> ],
        [ sh:path <http://dbpedia.org/ontology/architecture> ],
        [ sh:datatype rdf:langString ;
            sh:path <http://dbpedia.org/ontology/address> ],
        [ sh:path <http://dbpedia.org/ontology/NationalRegisterOfHistoricPlacesReferenceNumber> ],
        [ sh:path <http://dbpedia.org/ontology/architecturalStyle> ],
        [ sh:class <http://dbpedia.org/ontology/Architect> ;
            sh:path <http://dbpedia.org/ontology/architect> ],
        [ sh:datatype xsd:gYear ;
            sh:path <http://dbpedia.org/ontology/yearOfConstruction> ],
        [ sh:path <http://dbpedia.org/ontology/currentTenants> ],
        [ sh:datatype xsd:double ;
            sh:path <http://dbpedia.org/ontology/cost> ],
        [ sh:class <http://dbpedia.org/ontology/Country> ;
            sh:path <http://dbpedia.org/ontology/country> ],
        [ sh:path <http://dbpedia.org/ontology/inaugurationDate> ],
        [ sh:datatype xsd:double ;
            sh:path <http://dbpedia.org/ontology/floorArea> ],
        [ sh:class <http://dbpedia.org/ontology/Language> ;
            sh:path <http://dbpedia.org/ontology/language> ],
        [ sh:datatype xsd:string ;
            sh:path <http://dbpedia.org/ontology/buildingStartDate> ] ;
    sh:targetClass <http://dbpedia.org/ontology/Building> .

