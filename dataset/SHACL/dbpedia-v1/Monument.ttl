@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://shaclshapes.org/MonumentShape> a sh:NodeShape ;
    sh:property [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path <http://www.w3.org/2003/01/geo/wgs84_pos#long> ],
        [ sh:path foaf:name ],
        [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path <http://www.w3.org/2003/01/geo/wgs84_pos#lat> ],
        [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path <http://www.georss.org/georss/point> ],
        [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path rdfs:comment ],
        [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path <http://www.w3.org/2003/01/geo/wgs84_pos#geometry> ],
        [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path foaf:depiction ],
        [ sh:datatype rdf:langString ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path <http://dbpedia.org/ontology/abstract> ],
        [ sh:datatype xsd:integer ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path <http://dbpedia.org/ontology/wikiPageID> ],
        [ sh:class <http://dbpedia.org/ontology/Image> ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path <http://dbpedia.org/ontology/thumbnail> ],
        [ sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path rdfs:label ],
        [ sh:class <http://dbpedia.org/ontology/Place> ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path <http://dbpedia.org/ontology/location> ],
        [ sh:maxCount 1 ;
            sh:path foaf:homepage ],
        [ sh:datatype xsd:integer ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:path <http://dbpedia.org/ontology/wikiPageRevisionID> ],
        [ sh:class <http://dbpedia.org/ontology/Person> ;
            sh:path <http://dbpedia.org/ontology/designer> ] ;
    sh:targetClass <http://dbpedia.org/ontology/Monument> .

