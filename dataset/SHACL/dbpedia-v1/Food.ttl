        [ sh:minCount 1 ;
            sh:path <http://dbpedia.org/ontology/ingredient> ],
        [ sh:path <http://dbpedia.org/ontology/industry> ],
        [ sh:datatype xsd:string ;
            sh:path <http://dbpedia.org/ontology/cuisine> ],
        [ sh:class <http://dbpedia.org/ontology/Image> ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
